<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="carbon.zeroevents.Manual_User.LoginOptions">

    <Button
        android:id="@+id/manualSignUpBut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="193dp"
        android:padding="20dp"
        android:text="@string/manualSignUpBut" />


    <com.facebook.login.widget.LoginButton
        android:id="@+id/facebookLoginBut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/manualSignUpBut"
        android:layout_centerHorizontal="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="34dp"
        android:padding="20dp"
        android:text="@string/facebookLoginBut"/>



</RelativeLayout>
